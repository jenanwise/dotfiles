[color]
    diff = auto
    branch = auto
    interactive = auto
    status = always
    ui = always
[core]
    excludesfile = ~/.gitignore
    editor = vim

[alias]
    s = status
    st = status -s
    stf = ! (git status --short | cut -d\\   -f3)
    dfc = diff --cached
    dfw = diff --word-diff
    co = checkout
    lg = log --graph --pretty=format:'%Cred%h%Creset -%Cblue%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
    oneline = log --pretty=oneline
    names = log --name-only
    alog = log --abbrev-commit
    todo = grep --heading --line-number --break -e "FIXME" -e "TODO" -e "XXX"
    b = branch
    rev = rev-parse --short
    d = diff
    dom = diff origin/master
    rom = rebase -i origin/master
    set-personal = config user.email jenan@jenanwise.com
    sp = set-personal
[branch]
    autosetuprebase = always
[push]
    default = upstream
[github]
    user = jenanwise
[status]
    relativePaths = true
[user]
	useconfigonly = true
	name = Jenan Wise
[diff]
	compactionHeuristic = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = true
